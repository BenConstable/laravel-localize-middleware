{
  "name": "benconstable/laravel-localize-middleware",
  "description": "Configurable localization middleware for your Laravel >=5.1 application",
  "keywords": [
    "laravel",
    "localise",
    "localize",
    "middleware"
  ],
  "homepage": "https://github.com/BenConstable/laravel-localize-middleware",
  "license": "MIT",
  "authors": [
    {
      "name": "Ben Constable",
      "email": "ben@benconstable.co.uk",
      "homepage": "https://benconstable.co.uk",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "laravel/framework": "~5.1|^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.8",
    "mockery/mockery": "~0.9",
    "squizlabs/php_codesniffer": "~2.0"
  },
  "autoload": {
    "psr-4": {
      "BenConstable\\Localize\\": "src/Localize"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\BenConstable\\Localize\\": "tests/Localize"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.x-dev"
    },
    "laravel": {
      "providers": [
        "BenConstable\\Localize\\LocalizeServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "phpunit"
  }
}
